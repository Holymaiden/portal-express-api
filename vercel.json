{
  "outputDirectory": "dist",
  "builds": [{ "src": "/dist/*.js", "use": "@vercel/node" }],
  "rewrites": [{ "source": "/(.*)", "destination": "/dist" }],
  "buildCommand": "npm instal && npm run build && node  ./dist/index.js",
  "devCommand": "tsc --project ./tsconfig.build.json & node --trace-warnings ./dist/index.js"
}
